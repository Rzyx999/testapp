{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "outDir": "./dist/",
    "allowJs": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": false,
    "jsx": "react-jsx"
  },
  "ts-node": { 
    "compilerOptions": {
      "module": "commonjs"
    }
  }
}
